<view class="box">
	<view class="head">
		<image src="{{head}}"></image>
		<view>{{nickName}}</view>
		<view>{{bindName}}</view>
		<view wx:for="{{star}}" wx:key="index">
			<image src="{{starPic}}"></image>
		</view>
		<view class="myIcon">
			<view class="pics">
				<image class="icon" src=""></image>
				<view bindtap="goInvite">邀请好友</view>
			</view>
			<view class="pics">
				<image class="icon" src=""></image>
				<view bindtap="goWallet">我的钱包</view>
			</view>
			<view class="pics">
				<image class="icon" src=""></image>
				<view bindtap="goCuoupon">优惠券</view>
			</view>
			<view class="pics">
				<image class="icon" src=""></image>
				<view bindtap="goCredit">信用账期</view>
			</view>
		</view>
	</view>

	<!-- 我的服务 -->
	<view class="body">
		<view>我的服务</view>
		<view>
			<view class="pics">
				<image class="icon" src=""></image>
				<view  bindtap="goBank">银行卡管理</view>
			</view>
			<view class="pics">
				<image class="icon" src=""></image>
				<view bindtap="goExchange">发票报销</view>
			</view>
			<view class="pics">
				<image class="icon" src=""></image>
				<view bindtap="goAuth">我的认证</view>
			</view>
			<view class="pics">
				<image class="icon" src=""></image>
				<view bindtap="goStar">星级评定</view>
			</view>
			<view class="pics">
				<image class="icon" src=""></image>
				<view bindtap="goSuggestion">意见反馈</view>
			</view>
			<view class="pics">
				<image class="icon" src=""></image>
				<view bindtap="goService">客服服务</view>
			</view>
		</view>
	</view>

</view>